{
    "name": "selfVideoTs",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "node ace serve --watch",
        "build": "node ace build --production",
        "start": "node build/server.js",
        "lint": "eslint . --ext=.ts",
        "format": "prettier --write ."
    },
    "eslintConfig": {
        "extends": [
            "plugin:adonis/typescriptApp",
            "prettier"
        ],
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": [
                "error"
            ]
        }
    },
    "eslintIgnore": [
        "build"
    ],
    "prettier": {
        "trailingComma": "es5",
        "semi": false,
        "singleQuote": true,
        "useTabs": false,
        "quoteProps": "consistent",
        "bracketSpacing": true,
        "arrowParens": "always",
        "printWidth": 100
    },
    "devDependencies": {
        "@adonisjs/assembler": "^5.9.5",
        "@adonisjs/view": "^6.2.0",
        "@babel/core": "^7.20.12",
        "@babel/preset-env": "^7.20.2",
        "@japa/preset-adonis": "^1.2.0",
        "@japa/runner": "^2.2.2",
        "@symfony/webpack-encore": "^4.1.1",
        "@types/proxy-addr": "^2.0.0",
        "@types/source-map-support": "^0.5.6",
        "adonis-preset-ts": "^2.1.0",
        "eslint": "^8.31.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-adonis": "^2.1.1",
        "eslint-plugin-prettier": "^4.2.1",
        "file-loader": "^6.2.0",
        "less": "^4.1.3",
        "less-loader": "^11.1.0",
        "pino-pretty": "^9.1.1",
        "postcss-loader": "^7.0.2",
        "prettier": "^2.8.2",
        "sass": "^1.57.1",
        "sass-loader": "^13.2.0",
        "stylus": "^0.58.1",
        "stylus-loader": "^7.1.0",
        "typescript": "~4.6",
        "webpack": "^5.75.0",
        "webpack-cli": "^4.10.0",
        "youch": "^3.2.2",
        "youch-terminal": "^2.1.5"
    },
    "dependencies": {
        "@adonisjs/core": "^5.9.0",
        "@adonisjs/lucid": "^18.3.0",
        "@adonisjs/repl": "^3.1.11",
        "@adonisjs/session": "^6.4.0",
        "@adonisjs/shield": "^7.1.0",
        "bootstrap": "^5.3.0-alpha1",
        "luxon": "^3.2.1",
        "mysql2": "^2.3.3",
        "proxy-addr": "^2.0.7",
        "reflect-metadata": "^0.1.13",
        "source-map-support": "^0.5.21"
    }
