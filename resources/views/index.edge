<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>AdonisJS - A fully featured web framework for Node.js</title>
    @entryPointStyles('app')
    @entryPointScripts('app')
</head>

<body>
    <main>
        <div> 
            <div style="color: #900; float: left; width: 100%; clear: both;">
                @each(user in users)
                <p><a href="{{ user.url }}" target="_blank" style="color: #333">{{ user.name }}</a></p>
                @endeach
            </div>
            
            <div>
                @each(anchor in users.getUrlsForRange(1, users.lastPage))
                <a href="{{ anchor.url }}">
                    {{ anchor.page }}
                </a>
                @endeach
            </div>
        </div>
    </main>
</body>

</html>